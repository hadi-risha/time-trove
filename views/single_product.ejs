    <!-- js for add to cart text change -->
    <script>
        function toggleButtonText() {
            // Get the button element
            var button = document.getElementById('addToBagButton');

            // Check the current button text
            if (button.innerText === 'ADD TO BAG') {
                // If it's "ADD TO BAG," change to "GO TO CART"
                button.innerText = 'GO TO CART';
            } else {
                // If it's "GO TO CART," redirect to the cart page
                window.location.href = '/shopping-cart';
            }

            // Return false to prevent the default form submission behavior
            return false;
        }
    </script>
<!-- end of add to cart text change -->




